<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".subscribe.SubInfoActivity">

    <ImageButton
            android:id="@+id/subInfo_PreBtn"
            android:layout_width="50dp" android:layout_height="50dp"
            app:srcCompat="@drawable/ic_back_button" android:background="@android:color/transparent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.019"
            app:layout_constraintVertical_bias="0.03" android:layout_marginStart="24dp"/>
    <TextView
            android:id="@+id/subInfo_StartDayTxt"
            android:textSize="15dp" android:gravity="center_vertical"
            android:layout_width="520dp" android:layout_height="52dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.312"
            app:layout_constraintTop_toBottomOf="@+id/sub_textView16" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"/>
    <TextView
            android:id="@+id/subInfo_PeriodTxt"
            android:textSize="15dp" android:gravity="center_vertical"
            android:layout_width="520dp" android:layout_height="52dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.312"
            app:layout_constraintTop_toBottomOf="@+id/sub_textView15" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"/>
    <TextView
            android:id="@+id/subInfo_TypeTxt"
            android:textSize="15dp" android:gravity="center_vertical"
            android:layout_width="520dp" android:layout_height="52dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.312" app:layout_constraintTop_toBottomOf="@+id/sub_textView13"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"/>
    <TextView
            android:id="@+id/sub_textView7"
            android:text="구독 정보" android:gravity="center_vertical" android:textSize="20dp"
            android:layout_width="wrap_content" android:layout_height="50dp"
            app:layout_constraintStart_toEndOf="@+id/subInfo_PreBtn" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="28dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:id="@+id/sub_textView9"
            android:text="ID" android:textSize="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sub_textView7" android:layout_marginTop="28dp"
            app:layout_constraintHorizontal_bias="0.017" android:layout_marginStart="24dp"/>
    <TextView
            android:id="@+id/subInfo_IdTxt"
            android:textSize="15dp" android:gravity="center_vertical"
            android:layout_width="520dp" android:layout_height="52dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sub_textView9"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" app:layout_constraintHorizontal_bias="0.312"/>
    <TextView
            android:id="@+id/sub_textView13"
            android:text="신청 유형" android:textSize="20dp"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_IdTxt" android:layout_marginTop="45dp"
            app:layout_constraintHorizontal_bias="0.02" android:layout_marginStart="24dp"/>

    <TextView
            android:id="@+id/sub_textView15"
            android:text="신청 기간" android:textSize="20dp"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_TypeTxt" android:layout_marginTop="45dp"
            app:layout_constraintHorizontal_bias="0.02" android:layout_marginStart="24dp"/>
    <TextView
            android:id="@+id/sub_textView16"
            android:text="구독 시작날짜" android:textSize="20dp"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_PeriodTxt" android:layout_marginTop="45dp"
            app:layout_constraintHorizontal_bias="0.021" android:layout_marginStart="24dp"/>
    <TextView
            android:id="@+id/sub_textView17"
            android:text="신청 식단시간" android:textSize="20dp"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_StartDayTxt" android:layout_marginTop="45dp"
            app:layout_constraintHorizontal_bias="0.021" android:layout_marginStart="24dp"/>
    <CheckBox
            android:id="@+id/subInfo_MorningChkBox"
            android:text="아침 Morning" android:enabled="false"
            android:layout_width="520dp" android:layout_height="65dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.312" app:layout_constraintTop_toBottomOf="@+id/sub_textView17"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"/>
    <CheckBox
            android:id="@+id/subInfo_LunchChkBox"
            android:text="점심 Lunch" android:enabled="false"
            android:layout_width="520dp" android:layout_height="65dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.312"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_MorningChkBox"/>
    <CheckBox
            android:id="@+id/subInfo_DinnerChkBox"
            android:text="저녁 Dinner" android:enabled="false"
            android:layout_width="520dp" android:layout_height="65dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.312"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_LunchChkBox"/>
    <CheckBox
            android:id="@+id/subInfo_SnackChkBox"
            android:text="간식 Snack" android:enabled="false"
            android:layout_width="520dp" android:layout_height="65dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@+id/subInfo_DinnerChkBox"
            app:layout_constraintHorizontal_bias="0.312" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>
