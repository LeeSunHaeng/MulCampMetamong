<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".subscribe.SubMyMealsActivity">

    <ImageButton
        android:id="@+id/subMeal_PreBtn"
        android:layout_width="50dp" android:layout_height="50dp"
        app:srcCompat="@drawable/ic_back_button" android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.019"
        app:layout_constraintVertical_bias="0.03" android:layout_marginStart="24dp"/>
    <TextView
            android:id="@+id/subMeal_textView5"
            android:text="오늘의 식단" android:gravity="center_vertical" android:textSize="20dp"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toEndOf="@+id/subMeal_PreBtn"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="28dp"
            app:layout_constraintHorizontal_bias="0.0"/>

    <LinearLayout
            android:id="@+id/subMeal_linearLayout"
            android:orientation="vertical"
            android:layout_width="520dp" android:layout_height="610dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.312" app:layout_constraintTop_toBottomOf="@+id/subMeal_textView5"
            app:layout_constraintVertical_bias="0.264" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>